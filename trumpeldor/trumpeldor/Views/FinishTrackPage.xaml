<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="trumpeldor.Views.FinishTrackPage">
    <ContentPage.Content>
        <StackLayout Style="{DynamicResource mainStackLayout}">
            <Label x:Name="titleLabel" Text="congrdulation you finish the track with " Style="{DynamicResource lableStyle}"/>
            <Button x:Name = "continueButton" Text="continue to longer track" Clicked="Continiue_Button_Clicked" Style="{StaticResource largeButtonStyle}"/>
            <Button x:Name = "shareButton" Text="share" Clicked="Share_Button_Clicked" Style="{StaticResource largeButtonStyle}"/>
            <Button x:Name = "leadingTableButton" Text="Leading Table" Clicked="Leading_Table_Button_Clicked" Style="{StaticResource largeButtonStyle}"/>
            <Button x:Name = "feedbackButton" Text="feedback" Clicked="feedback_Button_Clicked" Style="{StaticResource largeButtonStyle}"/>
            <Button x:Name = "informationButton" Text="information" Clicked="information_Button_Clicked" Style="{StaticResource largeButtonStyle}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>